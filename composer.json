{
  "name": "lexhoy/despachos-search",
  "description": "Plugin para gestionar y buscar despachos de abogados integrado con Algolia",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "version": "2.2.0",
  "authors": [
    {
      "name": "JosÃ© Blanco",
      "email": "info@blancoyenbatea.com",
      "homepage": "https://blancoyenbatea.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "algolia/algoliasearch-client-php": "^4.19.0",
    "composer/installers": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Lexhoy\\Despachos\\": "includes/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
